1 REM III-34 - 2.4.4 Frequency Counter
10 FOR I=0 TO 34
20 READ A:POKE &H6000+I,A
30 NEXT
40 DATA &HE5,&HDB,&H03,&HDB,&H04,&HE6,
		&H04,&H28,&HFA,&HDB,&H02,&H5F,
		&HDB,&H03,&H57,&HDB,&H04,&HE6,
		&H04,&H28,&HFA,&HDB,&H02,&H6F,
		&HDB,&H03,&H67,&HED,&H52,&HEB,
		&HE1,&H73,&H23,&H72,&HC9
100 OUT &H00,&H65:CLS
110 DEF USR1=&H6000
120 T%=USR1(0)
130 IF T%>0 THEN T=T% ELSE T=T%+65536!
140 F=1000/(T*(614.1))
150 LOCATE 1,1
160 PRINT USING "#####,#";F;
170 PRINT " HZ":FOR I=0 TO 500:NEXT
180 GOTO 120
190 END
